find.mf(NCAAdf,.5,func=MostellerLARC)
find.mf <- function(df, mf = 1, func = BradleyTerryLARC, adj=1) {
while (func(df$Strength,df$WinsVersus,mf) == 0) {
mf = mf + 1
}
while (func(df$Strength,df$WinsVersus,mf) == Inf) {
mf = mf - 1
}
return(mf)
}
