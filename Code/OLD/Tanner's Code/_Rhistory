teams <- 10
games <- 25
VS.matrix <- function(teams, games) {
mm <- matrix(0,teams,teams)
nogo <- vector()
tt <- 1:teams^2
for (i in 1:teams) {
nogo <- c(nogo,teams*(i-1)+i)
}
tt <- tt[-which(tt==nogo)]
for (i in 1:games) {
sam <- sample(tt,1)
mm[sam] <- mm[sam] + 1
}
return(mm)
}
