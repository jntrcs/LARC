LARC.Posterior <- function(df, func = BradleyTerryLARC, mf = 1, adj=1) {
posterior <- func(df$Strength,df$WinsVersus,mf)
while (posterior == 0) {
mf = mf + adj
posterior <- func(df$Strength,df$WinsVersus,mf)
}
while (posterior == Inf) {
mf = mf - adj
posterior <- func(df$Strength,df$WinsVersus,mf)
}
return(c(posterior,mf))
}
